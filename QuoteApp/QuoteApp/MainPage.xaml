<?xml version="1.0" encoding="utf-8" ?>
<ContentPage xmlns="http://xamarin.com/schemas/2014/forms"
             xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
             x:Class="QuoteApp.MainPage">

    <StackLayout x:Name="ButtonStackLayout">
        <!-- Place new controls here -->
        <Label Text="This is Jimmy" 
           HorizontalOptions="Center"
           VerticalOptions="Start" />

        <Image x:Name="MyImage"/>

        <Button x:Name="RotationSliderPageNavigationButton"
                Text="Navigate to Rotation Slider"
                HorizontalOptions="Center"
                VerticalOptions="Center"
                Clicked="RotationSliderPageNavigationButton_OnClicked"/>

        <Button x:Name="QuoteItemPageNavigationButton"
                Text="Navigate to Quote Item Page"
                HorizontalOptions="Center"
                VerticalOptions="Center"
                Clicked="QuoteItemPageNavigationButton_OnClicked"/>

    </StackLayout>

</ContentPage>
